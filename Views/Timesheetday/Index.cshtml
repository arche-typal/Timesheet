@model IList<timesheet.Models.TimesheetDay>

@if (Model.Count == 0)
{
    <p>No timesheets!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Name</th>
        </tr>
        @foreach (var sheet in Model)
        {
            <tr>
                <td>@sheet.Name</td>
            </tr>
        }
    </table>
}



<p><a asp-controller="TimesheetDay" asp-action="Add">Add Day</a></p>